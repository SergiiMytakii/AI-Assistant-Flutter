{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "AI Assistant (dev)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "dev",
                  "-t",
                "lib/main_dev.dart"
            ]
        },
        {
            "name": "AI Assistant (prod)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "prod",
                "-t",
                "lib/main_prod.dart"
            ]
        },
        {
            "name": "AI Assistant (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor",
                "prod",
                  "-t",
                "lib/main_prod.dart"
            ]
        }
        ,
        {
            "name": "AI Assistant (web dev)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "-d",
                "chrome",
                "--dart-define",
                "FLAVOR=dev",
                "--web-renderer",
                "html",
                 "-t",
                "lib/main_dev.dart"
            ]
        },
        {
            "name": "AI Assistant (web prod)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "-d",
                "chrome",
                "--dart-define",
                "FLAVOR=prod",
                "--web-renderer",
                "html",
                 "-t",
                "lib/main_prod.dart"
            ]
        }
    ]
}